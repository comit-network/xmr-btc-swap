{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:event:allow-emit",
    "core:event:default",
    "clipboard-manager:allow-write-text",
    "shell:allow-open",
    "store:default",
    "process:default",
    "cli:allow-cli-matches",
    "dialog:default",
    "updater:default",
    "process:allow-restart"
  ]
}
